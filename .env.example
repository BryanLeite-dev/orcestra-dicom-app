# Database
DATABASE_URL=postgresql://user:password@localhost:5432/orcestra_dicom

# Server
NODE_ENV=development
PORT=3000

# OAuth Google - Server-side
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:3000/api/oauth/google/callback

# OAuth Google - Frontend (required for browser to initiate OAuth)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# OAuth Portal (Manus - legacy)
VITE_OAUTH_PORTAL_URL=https://api.manus.im
VITE_APP_ID=orcestra-dicom-app

# Frontend Optional
VITE_FRONTEND_FORGE_API_KEY=optional_api_key
VITE_FRONTEND_FORGE_API_URL=optional_api_url
VITE_ANALYTICS_ENDPOINT=optional_analytics_endpoint
VITE_ANALYTICS_WEBSITE_ID=optional_analytics_website_id

# JWT & Security
JWT_SECRET=your-jwt-secret-key-here
